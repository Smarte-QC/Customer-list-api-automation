		{
			"name": "lead-lists",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "37bb871d-f8b7-49c5-9d2b-e822c78f3d01",
						"exec": [
							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response schema for status and data fields is valid\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('object');\r",
							"    pm.expect(responseData).to.have.all.keys('status', 'data');\r",
							"\r",
							"    pm.expect(responseData.status).to.be.an('object');\r",
							"    pm.expect(responseData.status).to.have.all.keys('statusCode', 'requestId', 'message');\r",
							"    pm.expect(responseData.status.statusCode).to.be.a('number');\r",
							"    pm.expect(responseData.status.requestId).to.be.a('string');\r",
							"    pm.expect(responseData.status.message).to.be.a('string');\r",
							"\r",
							"    pm.expect(responseData.data).to.be.an('object');\r",
							"    pm.expect(responseData.data).to.have.all.keys('currentPage', 'leadLists', 'total', 'totalPages');\r",
							"    pm.expect(responseData.data.currentPage).to.be.a('number');\r",
							"    pm.expect(responseData.data.total).to.be.a('number');\r",
							"    pm.expect(responseData.data.totalPages).to.be.a('number');\r",
							"\r",
							"    pm.expect(responseData.data.leadLists).to.be.an('array');\r",
							"    responseData.data.leadLists.forEach(function(leadList) {\r",
							"        pm.expect(leadList).to.be.an('object');\r",
							"        pm.expect(leadList).to.have.all.keys('accountId', 'id', 'isDeleted', 'leadShares', 'listName', 'public');\r",
							"        pm.expect(leadList.accountId).to.be.a('number');\r",
							"        pm.expect(leadList.id).to.be.a('number');\r",
							"        pm.expect(leadList.isDeleted).to.be.a('boolean');\r",
							"        pm.expect(leadList.leadShares).to.be.an('array');\r",
							"        pm.expect(leadList.listName).to.be.a('string');\r",
							"        pm.expect(leadList.public).to.be.a('boolean');\r",
							"    });\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response time is less than 500ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"LeadLists is an array\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('object');\r",
							"    pm.expect(responseData.data.leadLists).to.be.an('array');\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"The listName must be a non-empty string in the leadLists array\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData).to.be.an('object');\r",
							"    pm.expect(responseData.data.leadLists).to.be.an('array').that.is.not.empty;\r",
							"\r",
							"    responseData.data.leadLists.forEach(function(leadList) {\r",
							"        pm.expect(leadList.listName).to.exist.and.to.be.a('string').and.to.have.lengthOf.at.least(1, \"Value should not be empty\");\r",
							"    });\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
